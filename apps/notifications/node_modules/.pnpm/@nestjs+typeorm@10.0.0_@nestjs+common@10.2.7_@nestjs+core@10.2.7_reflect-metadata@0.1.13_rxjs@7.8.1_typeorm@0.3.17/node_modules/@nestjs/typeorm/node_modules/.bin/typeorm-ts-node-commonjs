#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/typeorm@0.3.17_mysql2@3.6.2/node_modules/typeorm/node_modules:/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/typeorm@0.3.17_mysql2@3.6.2/node_modules:/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/typeorm@0.3.17_mysql2@3.6.2/node_modules/typeorm/node_modules:/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/typeorm@0.3.17_mysql2@3.6.2/node_modules:/Users/godson/Documents/mine/sleepr/apps/notifications/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.17_mysql2@3.6.2/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.17_mysql2@3.6.2/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
